<section class="contactPageInfo">
    <div class="contactPageHeading">
        <div class="contactPageLinks">
            <a class="ppp" href="/">Ana Səhifə</a> /
            <a class="ppp ppp-second" href="/contact">Əlaqə</a>
        </div>
    </div>

    <div class="contactPageCard">
        <div class="contactPageCardInfo">
            <img src="assets/img/phone-contact.svg" alt="avanqardups-call" />
            <h2>Əlaqə nömrələri</h2>
            <p >
                <a href="tel: +994 12 404 53 08">&nbsp +994 12 404 53 08 &nbsp</a>
                <br>
                <a href="tel: +994 50 310 77 16 ">&nbsp +994 12 310 77 16 &nbsp</a>
                <br>
                <a href="tel: +994 50 427 26 16 ">&nbsp +994 50 427 26 16 &nbsp</a>
            </p>
        </div>
        <div class="contactPageCardInfo">
            <img src="assets/img/loc-contact.svg" alt="avanqardups-location" />
            <h2>Ofis ünvanımız</h2>
            <a href="https://goo.gl/maps/cfLLMwiRtW5sL83q7" target="_blank">
                Ağ şəhər Y/M, 1-ci Yaşıl ada küçəsi, bina 7, mənzil 12</a>
        </div>
        <div class="contactPageCardInfo">
            <img src="assets/img/mail-contact.svg" alt="avanqardups-email" />
