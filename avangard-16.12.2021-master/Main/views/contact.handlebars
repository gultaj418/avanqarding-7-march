            <a href="mailto:office@aitms.net"> office@aitms.net </a>
        </div>
    </div>
</section>



<section  class="contactUs" id="contactIndex">
    <div class="contactUsFlex">
        <form class="contactUsInfo"  id="contactUs" >
            <h1>Bizimlə əlaqə saxlayın</h1>





            <p>
                Aşağıdakı əlaqə formunu dolduraraq suallarınızı, təklif və
                iradlarınızı bildirə bilərsiniz
            </p>
            <div class="formContactContain">
                <div class="smth">
                    <label for="fname">Adınız:</label>
                    <div id="nameResponsed" class="form-response"></div>


                    <input
                            class="contactFormElementStyle"
                            type="text"
                            id="fname"
                            name="name"
                            placeholder="Adınız"
                    />

                </div>

                <div class="smth">
                    <label for="email">Mail ünvanınız:</label>
                    <div id="emailResponsed" class="form-response"></div>
                    <input
                            class="contactFormElementStyle"
                            type="email"
                            id="email"
                            name="email"
                            placeholder="məs, adınız@shirketiniz.com"
                    />
                </div>
                <div class="smth">
                    <label for="phone">Telefon nömrəniz:</label>
                    <div id="phoneResponsed" class="form-response"></div>
                    <input
                            class="contactFormElementStyle"
                            type="tel"
                            id="phone"
                            name="phone"
                            placeholder="məs, +99412 345 67 89"
                    />
                </div>

                <div class="smth">
                    <label for="companyName">Şirkət adı:</label>
                    <div id="companyNameResponsed" class="form-response"></div>
                    <input
                            class="contactFormElementStyle"
                            type="text"
                            id="companyName"
                            name="companyName"
                            placeholder="Sizin şirkət"
                    />
                </div>

                <div class="smth">
                    <label for="movzu">Mövzu</label>

                    <select name="subject" id="movzu">
                        <option value="Məhsullar haqqında">Məhsullar haqqında</option>
                        <option value="Qiymət haqqında">Qiymət haqqında</option>
                        <option value="Müqavilə haqqında">Müqavilə haqqında</option>
                        <option value="Digər">Digər</option>
                    </select>

                </div>

                <div class="smth">
                    <label for="message">Mesajınız:</label>
                    <div id="messageResponsed" class="form-response"></div>
                    <input
                            class="contactFormElementStyle"
                            type="text"
                            id="message"
                            name="message"
                            placeholder="Mesajınız"
                    />
                    <br />
                    <button class="contactBtn" id="contactBtn" type="button">
                        Göndər
                    </button>

                </div>




            </div>

        </form>
        <div class="success-message">Your message has been sent!
            <svg id="success-icon" version="1.1" xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 32 32">
                <title>checkbox-checked</title>
                <rect fill="#2D79CF" x="0" y="0" width="32" height="32"></rect>
                <path fill="#fff" d="M28 0h-24c-2.2 0-4 1.8-4 4v24c0 2.2 1.8 4 4 4h24c2.2 0 4-1.8 4-4v-24c0-2.2-1.8-4-4-4zM14 24.828l-7.414-7.414 2.828-2.828 4.586 4.586 9.586-9.586 2.828 2.828-12.414 12.414z"></path>
            </svg>
        </div>
        <div class="contactUsImage">
            <img src="assets/img/contact-image.svg" alt="avanqardups-contact" />
        </div>
    </div>
</section>

